    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Server Debug",
            "restart": true,
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack:///./~/*": "${workspaceRoot}/node_modules/*",
                "webpack:///./*": "${workspaceRoot}/*",
                "webpack:///*": "*"
            },
            "outFiles": [
                "${workspaceRoot}/dist/server.js"
            ],
            "port": 9229
        }
    ]
}